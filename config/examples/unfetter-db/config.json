        "configKey": "killChains",
        "configValue": [
            {
                "name": "mitre-attack",
                "phase_names": [
                    "initial-access",
                    "execution",
                    "persistence",
                    "privilege-escalation",
                    "defense-evasion",
                    "credential-access",
